{
  "manifest_version": 3,
  "name": "Highlighter",
  "version": "1.0.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Highlighter"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
